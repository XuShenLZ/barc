<launch>
	<rosparam command ="load" file="$(find labs)/launch/ACC2018/module3/image_processing_parameters.yaml"/>
	<rosparam command ="load" file="$(find labs)/launch/ACC2018/module3/mpc_parameters.yaml"/>

	<!-- IMAGE PROCESSING -->
	<node 
		pkg="labs" 
		type="image_processing_lane_keeping.py" 
		name="image_processing_lane_keeping" 
		output="screen">
	</node>
	
	  

	<!-- HIGH LEVEL CONTROLLER - MPC -->
	<node 
		pkg="labs" 
		type="mpc_reference_tracking.jl" 
		name="mpc_reference_tracking" 
		output="screen">
	</node>

	<!-- LOW LEVEL CONTROLLER -->
	<node 
		pkg="labs" 
		type="low_level_PID_controller.py" 
		name="low_level_PID_controller"
        output = "screen">
    </node>

	<!-- KEYBOARD CONTROL -->
    <node
		pkg = "turtlesim"
		type = "turtle_teleop_key"
		name = "teleop_key"
		required = "true">
	</node>



</launch>
